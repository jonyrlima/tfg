	LOCAL.GOLDBALANCEADO = <FEVAL <FLOATVAL <ARGN1> / 1.8>>
ELSE
	LOCAL.GOLDBALANCEADO = <FEVAL <FLOATVAL <ARGN1> / 4>>

	// evitar descontinuidade da funcao (1000 / 1.8)
	IF (<dLOCAL.GOLDBALANCEADO> < 555)
		LOCAL.GOLDBALANCEADO = <EVAL {500 600}>
