[ITEMDEF i_anti_fcast]
ID=i_gold
NAME=antifcast

ON=@CREATE
ATTR=096
MORE1=<R5,8>

ON=@TIMER

IF <MORE1>
	REF69 <LINK.TAG.TARGET>
	IF <REF69.UID>
	IF <LINK.DISTANCE <REF69.UID>> > 1
		IF ((<REF69.P.X> > <LINK.P.X>)) && (<REF69.P.Y> > <LINK.P.Y>))
			LOCAL.DIR 3
		ELIF ((<REF69.P.X> > <LINK.P.X>)) && (<REF69.P.Y> < <LINK.P.Y>))
			LOCAL.DIR 1
		ELIF ((<REF69.P.X> < <LINK.P.X>)) && (<REF69.P.Y> < <LINK.P.Y>))
			LOCAL.DIR 7
		ELIF ((<REF69.P.X> < <LINK.P.X>)) && (<REF69.P.Y> > <LINK.P.Y>))
			LOCAL.DIR 5
		ELIF (<REF69.P.X> > <LINK.P.X>)
			LOCAL.DIR 2
		ELIF (<REF69.P.X> < <LINK.P.X>)
			LOCAL.DIR 6
		ELIF (<REF69.P.y> < <LINK.P.y>)
			LOCAL.DIR 0
		ELIF (<REF69.P.y> > <LINK.P.y>)
			LOCAL.DIR 4
		ENDIF
		IF <LINK.CANMOVE <dLOCAL.DIR>>
			doswitch <dLOCAL.dir>
				link.dir 0
				link.dir 1
				link.dir 2
				link.dir 3
				link.dir 4
				link.dir 5
				link.dir 6
				link.dir 7
			enddo
			doswitch <dLOCAL.dir>
				link.move n
				link.move ne
				link.move e
				link.move se
				link.move s
				link.move sw
				link.move w
				link.move nw
			enddo
		ENDIF
	ENDIF
	ENDIF
	MORE1 -=1
	TIMERD <R2,3>
	RETURN 1
ELSE
	LINK.TAG.ANTIFCAST
	REMOVE
	RETURN 0
ENDIF

