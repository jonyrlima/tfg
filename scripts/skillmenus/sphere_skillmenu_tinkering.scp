[SKILLMENU sm_bottles_empty]
Empty Bottles

ON=i_bottle_empty 10 Bottles (5 Ceramic)
IF !(<RESTEST 5 i_ingot_ceramic>)
SYSMESSAGE Voce nao possui Ceramic suficiente para fazer as Empty Bottles.
RETURN 1
ELSE
NEWITEM i_bottle_empty
new.amount 10
new.bounce
consume 5 i_ingot_ceramic
ENDIF


ON=i_bottle_empty 100 Bottles (50 Ceramic)
IF !(<RESTEST 50 i_ingot_ceramic>)
SYSMESSAGE Voce nao possui Ceramic suficiente para fazer as Empty Bottles.
RETURN 1
ELSE
NEWITEM i_bottle_empty
new.amount 100
new.bounce
consume 50 i_ingot_ceramic
ENDIF


ON=i_bottle_empty 500 Bottles (250 Ceramic)
IF !(<RESTEST 250 i_ingot_ceramic>)
SYSMESSAGE Voce nao possui Ceramic suficiente para fazer as Empty Bottles.
RETURN 1
ELSE
NEWITEM i_bottle_empty
new.amount 500
new.bounce
consume 250 i_ingot_ceramic 
ENDIF


