[TYPEDEF t_colored_forge]
ON=@DClick
	IF (<SRC.BLACKSMITHING> < <TAG.SKILL_NEEDED>)
		SRC.SYSMESSAGE @,,1 You must have <FVAL <TAG.SKILL_NEEDED>>% of Blacksmithing to coat an weapon with <TAG.ORE_NAME>.
		RETURN 1
	ENDIF

	IF (<SRC.FINDLAYER(79).ID>)
		SRC.SYSMESSAGE @,,1 You are already coating an weapon.
		SRC.FINDLAYER(79).REMOVE
		RETURN 1
	ENDIF


	SRC.SYSMESSAGE @,,1 Select the weapon you want to coat with <TAG.ORE_NAME>.
	SRC.TARGETF f_coat_weapon <UID>
	RETURN 1

[FUNCTION f_coat_weapon]
IF (<ARGV> != 1)
	RETURN 1
ENDIF

IF (<ARGO.TYPE> != t_weapon_fence) && (<ARGO.TYPE> != t_weapon_axe) && (<ARGO.TYPE> != t_weapon_mace_sharp) && (<ARGO.TYPE> != t_weapon_sword)
	SRC.SYSMESSAGE @,,1 The item selected is not an weapon.
	RETURN 1
ENDIF

IF !(STRMATCH(i_except*,<ARGO.BASEID>))
	SRC.SYSMESSAGE @,,1 Only Exceptional weapons can be coated.
	RETURN 1
ENDIF

IF (<ARGO.COLOR> != 0)
	SRC.SYSMESSAGE @,,1 This weapon is already coated.
	RETURN 1
ENDIF

IF (<ARGO.TOPOBJ> != <SRC.UID>)
	SRC.SYSMESSAGE @,,1 This weapon must be inside your bag.
	RETURN 1
endif

REF1 <ARGV[0]>
LOCAL.AMOUNT <EVAL (<STRARG <ARGO.RESOURCES>> * 4) / 10>

IF !(<SRC.RESTEST <LOCAL.AMOUNT> <REF1.TAG.RESOURCE_NEEDED>>)
	SRC.SYSMESSAGE You don't have <DLOCAL.AMOUNT> <REF1.TAG.ORE_NAME> ingots.
	RETURN 1
ENDIF

SRC.NEWITEM=i_mem_coating_weapon
NEW.LINK=<SRC.UID>
NEW.TAG.WEAPON_UID=<ARGO.UID>
NEW.TAG.AMOUNT=<LOCAL.AMOUNT>
NEW.TAG.RESOURCE_NEEDED=<REF1.TAG.RESOURCE_NEEDED>
NEW.TAG.ORE_NAME=<REF1.TAG.ORE_NAME>
NEW.TAG.COLOR=<REF1.COLOR>
NEW.TAG.LEVEL_ATTRIBUTE=<REF1.TAG.LEVEL_ATTRIBUTE>
NEW.TAG.LEVEL_ATTRIBUTE_BONUS=<REF1.TAG.LEVEL_ATTRIBUTE_BONUS>

NEW.EQUIP
NEW.TIMER=1
NEW.ATTR=082

[ITEMDEF i_mem_coating_weapon]
ID=0181d
NAME=Coating weapon
LAYER=79
TYPE=t_script
WEIGHT=0

ON=@CREATE
	ATTR=080
	TIMER=1
	LAYER=79

ON=@TIMER

IF ((<LINK.FLAGS>&statf_hidden) || (<LINK.FLAGS>&statf_invisible)) && !(<LINK.ISGM>)
	LINK.sysmessage You were revealed by coating an weapon.
	LINK.FLAGS <LINK.FLAGS>&~statf_hidden // tira hid
	LINK.FLAGS <LINK.FLAGS>&~statf_invisible // tira invis
ENDIF

If (<LINK.FLAGS> & STATF_WAR)
	LINK.SYSMESSAGE @,,1 You cant coat while in that state.
	REMOVE
	RETURN 0
ENDIF

REF1 <TAG.WEAPON_UID>

DORAND 4
  LINK.SAY You are coating <REF1.NAME>
  LINK.SAY You keep coating <REF1.NAME>
  LINK.SAY It is too hot to coat <REF1.NAME>
  LINK.SAY <REF1.NAME> is almost coated
ENDDO

MORE <MORE>+1

IF <MORE> < <R3,4>
	TIMER 1
	RETURN 1
ENDIF

IF !(<LINK.RESTEST <TAG.AMOUNT> <TAG.RESOURCE_NEEDED>>)
	LINK.SYSMESSAGE You don't have <DTAG.AMOUNT> <TAG.ORE_NAME> ingots.
	RETURN 1
ENDIF

LINK.CONSUME <TAG.AMOUNT> <TAG.RESOURCE_NEEDED>
REF1.COLOR <TAG.COLOR>
REF1.TAG.LEVEL_ATTRIBUTE <TAG.LEVEL_ATTRIBUTE>
REF1.TAG.LEVEL_ATTRIBUTE_BONUS <TAG.LEVEL_ATTRIBUTE_BONUS>
REF1.NAME <TAG.ORE_NAME> <REF1.NAME>

LINK.SYSMESSAGE @,,1 You finished coating <REF1.NAME>.

RETURN 0

