[typedef t_blood_weapon]
on=@equip
	src.events +e_blood_weapon

on=@unequip
	src.events -e_blood_weapon

[events e_blood_weapon]
on=@hit
	IF (<src.npc> == brain_dragon)
			argn1 = <eval <argn1>*2>
		if (<r1,3> == 1)
			SRC.SOUND=SND_SPELL_POISON
			src.effect 3,i_fx_glow_spike,1,20,20,0
		endif
	endif

[typedef t_silver_weapon]
on=@equip
	src.events +e_silver_weapon

on=@unequip
	src.events -e_silver_weapon

[events e_silver_weapon]
on=@hit
	IF (strmatch(*mummy*,<src.baseid>)) || (strmatch(*liche*,<src.baseid>)) || (strmatch(*skeleton*,<src.baseid>)) || (strmatch(*zombie*,<src.baseid>)) || (c_m_ghoul == <src.baseid>) || (c_m_ghost == <src.baseid>) || (c_m_wraith == <src.baseid>) || (c_spectre == <src.baseid>) || (c_rotting_corpse == <src.baseid>) || (c_grim_reaper == <src.baseid>)
			argn1 = <eval <argn1>*2>
		if (<r1,3> == 1)
			SRC.SOUND=SND_SPELL_POISON
			src.effect 3,i_fx_glow_spike,1,20,20,0
		endif
	endif

