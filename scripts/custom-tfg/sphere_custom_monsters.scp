		NEW.KARMA=0
		NEW.NPC=BRAIN_ANIMAL
		new.p <p>
		new.homedist 1
		new.update
		ref1 = <new.uid>
		ref1.timerf 10,say @38 DONT WORRY MY BELOVED CREATURE, EVERYTHING SHALL BE FINE!
		ref1.timerf 20,say @61 YOUR POWER HAS BEEN RESTABLISHED
		ref1.timerf 23,say @38 KILL THEM ALL!
		ref1.timerf 25,remove

