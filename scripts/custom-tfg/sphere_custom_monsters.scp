[FUNCTION f_reset_hourly_prisma]
VAR.SPAWN_HOURLY_PRISMA 1

FORINSTANCES c_m_prisma_dragon
	IF (<EVAL <SERV.TIME> - <TAG0.SPAWN_TIME>> >= 72000) && (<EVAL <SERV.TIME> - <TAG0.LAST_HIT_TIME>> >= 1200)
		SERV.LOG Removing decayed <NAME> at <P>.
		REMOVE
	ENDIF
ENDFOR

FORINSTANCES c_m_prisma_daemon
	IF (<EVAL <SERV.TIME> - <TAG0.SPAWN_TIME>> >= 72000) && (<EVAL <SERV.TIME> - <TAG0.LAST_HIT_TIME>> >= 1200)
		SERV.LOG Removing decayed <NAME> at <P>.
		REMOVE
	ENDIF
ENDFOR

[FUNCTION f_spawn_hourly_prisma]
IF (<VAR0.SPAWN_HOURLY_PRISMA>)
	LOCAL.PARAGON_HOUR = <DVAR0.PARAGON_HOUR_<f_paragon_hour>>
	LOCAL.POI_LOCATION <f_get_random_location <DEF.PARAPOI_<DLOCAL.PARAGON_HOUR>>>

	SERV.NEWNPC <EVAL {c_m_prisma_dragon 1 c_m_prisma_daemon 1}>
	NEW.P <LOCAL.POI_LOCATION>
	NEW.HOME <NEW.P>
	NEW.TAG.SPAWN_TIME <SERV.TIME>
	NEW.HOMEDIST 6
	NEW.UPDATE

	SERV.LOG Spawning <NEW.NAME> at <NEW.P> - (<NEW.REGION.NAME>) - POI.

	VAR.SPAWN_HOURLY_PRISMA
ENDIF

