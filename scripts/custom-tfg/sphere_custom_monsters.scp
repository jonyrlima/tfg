	IF (<EVAL <SERV.TIME> - <VAR0.LAST_ARCTIC_OGRE_FACE_DROP>> > 25920000) && !(<R4>)
		VAR.LAST_ARCTIC_OGRE_FACE_DROP <SERV.TIME>

		SERV.LOG Creating a new Arctic Ogre Face.
		SERV.NEWITEM i_artic_ogre_face
		NEW.CONT <UID>
	ENDIF

