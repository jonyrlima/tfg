QUEST_CARPENTRY_COPPER_COUNT 20000
QUEST_CARPENTRY_BRONZE_COUNT 15000
QUEST_CARPENTRY_SHADOW_COUNT 12500
QUEST_CARPENTRY_AGAPITE_COUNT 10000
QUEST_CARPENTRY_ROSE_COUNT 7500
QUEST_CARPENTRY_SILVER_COUNT 5000

[FUNCTION f_carpentry_quest_consume]
IF (<ARGV> != 4)
	RETURN 1
ENDIF

LOCAL.RECIPE_SCROLL_UID <ARGV[0]>
LOCAL.BASE_ID <ARGV[1]>
LOCAL.COUNT <ARGV[2]>
LOCAL.TAG <ARGV[3]>

IF (!(<ARGO.TOPOBJ>==<SRC.UID>) && (!<SRC.CANSEELOS <ARGO.UID>>)) || (<ARGO.ATTR> & attr_move_never)
	SRC.SYSMESSAGE @,,1 <DEFMSG.msg_key_targ_reach>
	RETURN 1
ENDIF

IF (<ARGO.BASEID> != <LOCAL.BASE_ID>)
	SRC.SYSMESSAGE @,,1 This is not the right item.
	RETURN 1
ENDIF

IF (<ARGO.AMOUNT> > <LOCAL.COUNT>)
	SRC.SYSMESSAGE @,,1 This is more than the amount needed.
	RETURN 1
ENDIF

UID.<LOCAL.RECIPE_SCROLL_UID>.TAG0.<LOCAL.TAG> += <ARGO.AMOUNT>

IF (<ARGO.AMOUNT> < <LOCAL.COUNT>)
	SRC.SYSMESSAGE @,,1 Now I need more <EVAL <LOCAL.COUNT> - <ARGO.AMOUNT>> <ARGO.NAME>s!
ELSE
	SRC.F_GM_QUEST_SKILL_INCREASE 11,<LOCAL.BASE_ID>
ENDIF

ARGO.REMOVE

[EOF]