[ITEMDEF i_map_train]
ID=i_scroll_Blank
NAME=Training Map
VALUE=1
WEIGHT=0.1

ON=@CREATE
COLOR 030

[FUNCTION f_mapmake]
REF69 <ARGS>
IF <CTAG0.USINGMAP>
SYSMESSAGE You are already writing a map.
ELSE
REF69.MORE -=1
timerf 2,f_mapdone
SRC.CTAG.USINGMAP 1
ENDIF

[function f_mapdone]
IF <R800> < <SRC.CARTOGRAPHY>
SRC.SYSMESSAGE You make a new training map.
SERV.NEWITEM i_map_train
NEW.CONT <SRC.UID>
Skillgain 12 1
ELSE
SRC.SYSMESSAGE You fail to make anything.
ENDIF
SRC.CTAG.USINGMAP 0

[TYPEDEF t_map_train]
ON=@CLICK
MESSAGE <NAME> (<MORE>/<dTAG0.AMOUNT>)
RETURN 1

ON=@DCLICK
IF <MORE>==0
REMOVE
ELSE
SRC.f_mapmake, <UID>
ENDIF
RETURN 1

