// Ore colored armor

[TYPEDEF t_shield]
ON=@CLICK
IF (<SRC.ISGM>)
	IF (<SRC.PARRYING> < 1000)
		message @0 [Armor : <FVAL (<dARMOR>*<src.parrying>)/100> - <dARMOR>]
	ELIF (<SRC.PARRYING> > 1000)
		message @0 [Armor : <FVAL (<dARMOR>*<src.parrying>)/100>]
	ELSE
		message @0 [Armor : <dARMOR>]
	ENDIF
ENDIF

ON=@Damage
	IF (<R6>)
		RETURN 1
	ENDIF

[TYPEDEF t_armor]
//Plate : 0.4
//Legs : 0.25
//Arms : 0.1
//Gorget : 0.05
//Gloves : 0.1
//Helm : 0.1
ON=@CLICK
IF (<SRC.ISGM>)
	IF (<LAYER> == 6 ) || (<LAYER> == 19 ) || (<LAYER> == 7 )
		message @0 [Armor : <FVAL (<dARMOR>*0.1)>]
	ELIF (<LAYER> == 10 )
		message @0 [Armor : <FVAL (<dARMOR>*0.05)/10>]
	ELIF (<LAYER> == 24 )
		message @0 [Armor : <FVAL (<dARMOR>*0.25)/10>]
	ELIF (<LAYER> == 13 )
		message @0 [Armor : <FVAL (<dARMOR>*0.4)>]
	ENDIF
ENDIF

