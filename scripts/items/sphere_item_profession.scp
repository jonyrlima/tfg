[FUNCTION f_vet_final]

TAG.ISUSINBAND

IF <HITS> < 1
	RETURN 1
ENDIF
SKILLGAIN 39 1
REF69=<ARGV[1]>

IF <VETERINARY>  > RAND(1100)
	CONSUME 1 i_bandage
	SERV.NEWITEM i_bandage_bloody
	NEW.CONT <UID>
	IF (<ARGV[0]>)
		IF <EVAL (<REF69.HITS> + <ARGV[0]>)> > <REF69.MAXHITS>
			TAG.ISUSINBAND
			SYSMESSAGE Voce curou <eval (<REF69.MAXHITS> - <REF69.HITS>)> hitpoints de <REF69.NAME>
			REF69.HITS = <REF69.MAXHITS>
		ELSE
			TAG.ISUSINBAND
			REF69.HITS += <EVAL <ARGV[0]>>
			SYSMESSAGE Voce curou <EVAL <ARGV[0]>> hitpoints de <REF69.NAME>
		ENDIF
	ELSE
		TAG.FINGERSLIP
		TAG.ISUSINBAND
	ENDIF
ELSE
	TAG.ISUSINBAND
	SYSMESSAGE Voce aplicou as bandagens porem elas nao adiantaram.
	CONSUME 1 i_bandage
	RETURN 1
ENDIF











