	LOCAL.HEALTARG <ARGO.UID>
	TAG.ISUSINBAND 1
	TIMERF 3, f_vet_final, <LOCAL.HEALAMOUNT>, <LOCAL.HEALTARG>
ELIF (<ARGO.ISPLAYER>)
	VAR.HEALMIN= ((<SRC.HEALING>/60)  + 5
	VAR.HEALMAX= ((<SRC.HEALING>/30) + 10
	LOCAL.HEALAMOUNT <EVAl RAND(<EVAL <VAR.HEALMIN>>, <EVAL <VAR.HEALMAX>>)>
	IF <ARGO.HITS> >=<ARGO.MAXHITS>
		if (<isempty <argo.tag0.name.alt>>)
			SYSMESSAGE <ARGO.NAME> nao precisa ser curado.
		else
			SYSMESSAGE <argo.tag0.name.alt> nao precisa ser curado.
		endif
		TAG.ISUSINBAND
		RETURN 1
	ENDIF
	LOCAL.HEALTARG <ARGO.UID>

	IF ( (<ARGO.ISPLAYER>) && (<ARGO.FLAGS>&02) )
		IF ((<HEALING> > 850) && (<ANATOMY> > 850))
			IF <RESCOUNT(i_bandage)> > 80

				ref1 <uid> //caster
				ref2 <argo.uid>	//tobe resurrected
				if (<ref2.f_can_i_be_resurrected> == 0)
					ref2.smsg @,,1 You cant be resurrected on that tile.
					ref1.smsg @,,1 <ref2.name> can't be resurrected over there. //'
					return 1
				endif

				if (<isempty <argo.tag.name.alt>>)
					if !(<tag0.name.alt>)
						SAY <NAME> tentando ressuscitar <ARGO.NAME>
					else
						SAY <tag0.name.alt> tentando ressuscitar <argo.name>
					endif
				else
					if !(<tag0.name.alt>)
						SAY <NAME> tentando ressuscitar <argo.tag0.name.alt>
					else
						say <tag0.name.alt> tentando ressuscitar <argo.tag0.name.alt>
					endif
				endif
				CONSUME 80 i_bandage
				TAG.ISUSINBAND 1
				f_ress_bandage <LOCAL.HEALTARG>
				RETURN 1
			ELSE
				TAG.ISUSINBAND
				SYSMESSAGE Voce nao possui 80 bandagens para ressuscitar alguem.
				RETURN 1
			ENDIF
		ELSE
			TAG.ISUSINBAND
			SYSMESSAGE Voce nao possui as skills necessarias para tentar ressuscitar alguem.
			RETURN 1
		ENDIF
	ELSE
		TAG.ISUSINBAND 1
		//local.tempobands = <r5,6>
		antiautomsgbands
		TIMERF <r1,4>, antiautomsgbands
		TIMERF 5, f_heal_final, <LOCAL.HEALAMOUNT>, <LOCAL.HEALTARG>
	ENDIF
ELSE
	SYSMESSAGE Voce nao pode usar bandagens em <ARGO.NAME>
ENDIF

[FUNCTION f_can_i_be_resurrected]
// if (<isevent.E_ARENA_1X1>)
// 	return 0
// endif

if (<isneartype t_door_locked 0>)
	foritems 1
		if (<type> == t_door_locked) && (<timer> < 0) && (<REGION.ISEVENT.r_house_private>)
			return 0
		endif
	endfor

endif
return 1

[FUNCTION f_ress_bandage]

IF <HITS> < 1
	TAG.ISUSINBAND
	RETURN 1
ENDIF

IF ((<FLAGS>&04) || (<FLAGS>&02))
	TAG.ISUSINBAND
	SYSMESSAGE Voce nao pode ressuscitar alguem neste estado.
	RETURN 1
ENDIF


REF69=<ARGS>

IF !(<CANSEELOS <REF69.UID>>)
	TAG.ISUSINBAND
	SYSMESSAGE Voce nao consegue enxergar o seu alvo.
	RETURN 1
ENDIF

IF (<DISTANCE <REF69>> > 3)
	TAG.ISUSINBAND
	SYSMESSAGE Voce esta muito distante de seu alvo.
	RETURN 1
ENDIF

IF <eval ((<ANATOMY> + <HEALING>)/2)> > RAND(1050)
	IF !(<SRC.REGION>&012)
		SERV.NEWITEM I_RESSB_TIMER
		NEW.LINK <UID>
		NEW.TIMER 3
		NEW.CONT <REF69>
		SERV.NEWITEM I_RESSANDO
		NEW.TIMER 3
		NEW.CONT <UID>
		//REF69.RESURRECT
		SYSMESSAGE Voce esta tentando ressuscitar seu alvo...
	ELSE
		SYSMESSAGE Voce nao pode ressuscitar neste local.
		TAG.ISUSINBAND
	ENDIF
ELSE
		TAG.ISUSINBAND
		SYSMESSAGE Voce falhou ao tentar ressuscitar seu alvo.
ENDIF


