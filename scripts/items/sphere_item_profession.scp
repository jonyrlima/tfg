[FUNCTION f_heal_final]
if <ctag0.healfinalplus>
	ctag0.healfinalplus =
	//s <name> bandage delayed, message fake enviada. Vai clicar?
	antiautomsgbands
	TIMERF 3, f_heal_final, <argv[0]>, <argv[1]>
	return 1
endif

TAG.ISUSINBAND


IF (<REF69.FINDID.i_poison_effect.UID>)
	SYSMESSAGE @,,1 Your target is poisoned and cant be healed.
	TAG.FINGERSLIP
	RETURN 1
ENDIF

IF <HITS> < 1
RETURN 1
ENDIF
IF <HEALING> < 1000
		HEALING += 1
ENDIF

REF69=<ARGV[1]>
IF (<ref69.hits> <= <ref69.maxhits>) && (<eval <HEALING>> > RAND(700))
	TAG.ISUSINBAND

	IF !(<RESTEST i_bandage>)
		SYSMESSAGE Voce deve ter bandagens na sua mochila.
		RETURN 1
	ENDIF

	CONSUME 1 i_bandage
	SERV.NEWITEM i_bandage_bloody
	NEW.CONT <UID>
	IF (<ARGV[0]>)
		IF <EVAL (<REF69.HITS> + <ARGV[0]>)> > <REF69.MAXHITS>
			TAG.FINGERSLIP
				if (<ctag0.antiautovigilance> > <serv.time>)
					if <r0,1>
						SYSMESSAGE @,,1 Voce curou <eval ( <REF69.MAXHITS> - <REF69.HITS> )> hitpoints de <REF69.NAME>.
					endif
				else
					if (<r0,99> > 9)
						SYSMESSAGE @,,1 Voce curou <eval ( <REF69.MAXHITS> - <REF69.HITS> )> hitpoints de <REF69.NAME>.
					endif
				endif
			REF69.HITS = <REF69.MAXHITS>
			CTAG0.LASTHEAL = <serv.time>
			REF69.EFFECT 3,i_fx_HEAL_EFFECT,9,15,1
		ELSE
			TAG.FINGERSLIP
			REF69.HITS += <EVAL <ARGV[0]>>
			CTAG0.LASTHEAL = <serv.time>
				if (<ctag0.antiautovigilance> > <serv.time>)
					if <r0,1>
						SYSMESSAGE @,,1 Voce curou <EVAL <ARGV[0]>> hitpoints de <REF69.NAME>.
					endif
				else
					if (<r0,99> > 9)
						SYSMESSAGE @,,1 Voce curou <EVAL <ARGV[0]>> hitpoints de <REF69.NAME>.
					endif
				endif
			REF69.EFFECT 3,i_fx_HEAL_EFFECT,9,15,1
		ENDIF
	ELSE
		TAG.FINGERSLIP
		if (<r0,99> > 9)
			SYSMESSAGE @,,1 Voce nao curou nada de <REF69.NAME>.
		endif
	ENDIF
	return 1
ENDIF
	TAG.ISUSINBAND
	TAG.FINGERSLIP
	if (<r0,99> > 9)
		SYSMESSAGE @,,1 Voce aplicou as bandagens porem elas nao adiantaram.
	endif
	CONSUME 1 i_bandage
	SERV.NEWITEM i_bandage_bloody
	NEW.CONT <UID>
	RETURN 1

[FUNCTION F_VET_UNDEAD]
TAG.FINGERSLIP
TAG.ISUSINBAND
IF <R5> == 1
	SRC.SYSMESSAGE @,,1 You have failed to do that...
	local.cons <r20,30>
	consume <dlocal.cons> I_BANDAGE
	serv.newitem i_bandage_bloody,<dlocal.cons>,<src.FINDLAYER.21.UID>
	RETURN 1
ENDIF

