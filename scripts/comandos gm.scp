// GM ADD

[FUNCTION gmadd]
IF (<SRC.ACCOUNT.PLEVEL> >= 4)
	IF <ISEMPTY <ARGS>>
		SRC.SYSMESSAGE Utilize: .gmadd (item)
		RETURN 1
	ENDIF

	IF (<ARGS> == 0)
		SRC.SYSMESSAGE Item inexistente
		RETURN 1
	ENDIF

	SRC.NEWITEM <ARGS>
	NEW.TAG.ADDFOR = <SRC.UID>
	NEW.BOUNCE
ENDIF
RETURN 1

// GM STONE

[FUNCTION gmstone]
	IF (<SRC.ACCOUNT.PLEVEL> >= 2)
		IF (<SRC.UID> == <UID>)
			SRC.SYSMESSAGE Quem Voce Deseja Stonar?
			TARGETF f_gm_stone
		ENDIF
	ENDIF
	RETURN 1


[FUNCTION f_gm_stone]
	IF (<SRC.ACCOUNT.PLEVEL> >= 2)
		SRC.TARG = <ARGO.UID>
		IF (<ARGO.UID>)
			IF <SRC.TARG.ISITEM>
				RETURN 1
			ELSEIF <SRC.TARG.ISCHAR>
				SRC.TARG.STONE
			ENDIF
		ENDIF
	ENDIF
	RETURN 1

// GM COLOR

[FUNCTION gmcolor]
	IF (<SRC.ACCOUNT.PLEVEL> >= 4)
		IF <ISEMPTY <ARGS>>
			SRC.SYSMESSAGE Utilize: .gmcolor (color)
			RETURN 1
		ENDIF
		IF (<SRC.UID> == <UID>)
			SRC.SYSMESSAGE Color?...
			TARGETF f_gm_color <ARGS>
		ENDIF
	ENDIF
	RETURN 1


[FUNCTION f_gm_color]
IF !<ISEMPTY <ARGS>>
	IF (<SRC.ACCOUNT.PLEVEL> >= 4)
		SRC.TARG = <ARGO.UID>
		IF (<ARGO.UID>)
			IF <SRC.TARG.ISITEM>
				SRC.TARG.COLOR <ARGS>
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

// GM NAME

[FUNCTION gmname]
	IF (<SRC.ACCOUNT.PLEVEL> >= 4)
		IF <ISEMPTY <ARGS>>
			SRC.SYSMESSAGE Utilize: .gmname (NOME)
			RETURN 1
		ENDIF
		IF (<SRC.UID> == <UID>)
			SRC.SYSMESSAGE Name?...
			TARGETF f_gm_name <ARGS>
		ENDIF
	ENDIF
	RETURN 1


[FUNCTION f_gm_name]
IF !<ISEMPTY <ARGS>>
	IF (<SRC.ACCOUNT.PLEVEL> >= 4)
		SRC.TARG = <ARGO.UID>
		IF (<ARGO.UID>)
			IF <SRC.TARG.ISITEM>
				SRC.TARG.NAME <ARGS>
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

[FUNCTION b]
IF (<SRC.ACCOUNT.PLEVEL>= 2)
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Cons] <SRC.NAME>: <ARGS>
ELSEIF (<SRC.ACCOUNT.PLEVEL>= 3)
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Seer] <SRC.NAME>: <ARGS>
ELSEIF (<SRC.ACCOUNT.PLEVEL>= 4)
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Gm] <SRC.NAME>: <ARGS>
ELSEIF (<SRC.ACCOUNT.PLEVEL>= 5)
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Dev] <SRC.NAME>: <ARGS>
ELSEIF (<SRC.ACCOUNT.PLEVEL>= 6)
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Admin] <SRC.NAME>: <ARGS>
ELSE
	SRC.SERV.ALLCLIENTS SYSMESSAGE @0790 [Owner] <SRC.NAME>: <ARGS>
ENDIF

