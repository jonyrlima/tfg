[FUNCTION fecha_gate_ffa]
if <tag0.can_i_use_event_cmd>
	forinstances i_gate_ffa
		remove
	endfor
	set_event_gate_walls 0
	forinstances i_anuncia_ffa
		remove
	endfor
	sysmessage @,,1 Gates fechados.
