	sysmessage @,,1 Voce nao pode usar este comando.
endif
return 1

[FUNCTION stop_ffa]
if <tag0.can_i_use_event_cmd>
	stop_ffa_op
else
	sysmessage @,,1 Voce nao pode usar este comando.
endif
return 1

