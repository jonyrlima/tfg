[CHARDEF c_power_scroll_specialist]
ID=c_man
NAME=Fenrir
TSPEECH=spk_power_scroll_specialist

ON=@Create
   TITLE=the Power Scroll Specialist
   COLOR=colors_skin
   NPC=brain_human
   STR={66 77}
   DEX={75 100}
   INT={45 55}
   flags = <flags>&statf_invul

   DETECTINGHIDDEN={20.0 30.0}
   FENCING={20.0 30.0}
   ITEMID={50.0 75.0}
   LOCKPICKING={50.0 75.0}
   WRESTLING={20.0 30.0}

   ITEMNEWBIE=i_hair_long
   COLOR=0
   ITEMNEWBIE=i_beard_long
   COLOR=match_hair
   ITEM=random_shirts_human
   COLOR=colors_all
   ITEM=random_pants_human
   COLOR=colors_all
   ITEM=random_footwear_human
   COLOR=colors_all
   ITEM=random_hats
   COLOR=colors_all

ON=@ReceiveItem
	IF (<ARGO.TYPE> != t_container)
		DORAND 2
			MESSAGE I only accept a bag containing at least two different power scrolls.
			MESSAGE Arrrgh, gimme a bag containing two or more power scrolls, jerk.
		ENDDO

		RETURN 1
	ENDIF

	LOCAL.SCROLLS_COUNT 0
	REF1 <UID>

	FORCONT <ARGO.UID> 10
		IF (<BASEID> != i_power_scroll)
			REF1.MESSAGE What did I say? The bag must contain power scrolls only!
			RETURN 1
		ENDIF

		IF (<AMOUNT> > 1) || (<LOCAL.SCROLL_<MORE1>> != 0)
			REF1.MESSAGE I don't want more than one scroll of each type, damn!
			RETURN 1
		ENDIF

		LOCAL.SCROLLS_COUNT += 1

		LOCAL.SCROLL_<MORE1> = 1
	ENDFOR

	IF (<LOCAL.SCROLLS_COUNT> < 2)
		MESSAGE You wanna cheat on me? I need at least two different power scrolls, you idiot!
		RETURN 1
	ENDIF

	LOCAL.RAND <R<DEF.WORK_MIN>,<DEF.WORK_MAX>>
	LOCAL.NEW_WORK_SKILL=<DEF.WORK<DLOCAL.RAND>>
	LOCAL.MAX_LOOP 250
	LOCAL.I 0

	WHILE (<LOCAL.I> < <LOCAL.MAX_LOOP>) && (<LOCAL.SCROLLS_COUNT> < <DEF.WORK_MAX>) && (<LOCAL.SCROLL_<LOCAL.NEW_WORK_SKILL>> != 0)
		LOCAL.RAND <R<DEF.WORK_MIN>,<DEF.WORK_MAX>>
		LOCAL.NEW_WORK_SKILL=<DEF.WORK<DLOCAL.RAND>>

		LOCAL.I += 1
	ENDWHILE

	SERV.NEWITEM i_power_scroll
	NEW.MORE1=<LOCAL.NEW_WORK_SKILL>
	NEW.NAME=<SERV.SKILL.<LOCAL.NEW_WORK_SKILL>.NAME> Power Scroll
	NEW.CONT <SRC.FINDLAYER.layer_pack>

	MESSAGE Here you go, <SRC.NAME> ! Thank you.

	SERV.LOG <SRC.NAME> has obtained a <NEW.NAME> from the power scroll specialist. Loops: <dLOCAL.I>

	ARGO.REMOVE

	RETURN 1

